{
  "name": "p5js-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn watchCSS & node ./bin/www",
    "build": "yarn run buildCSS",
    "buildCSS": "stylus src/stylesheets/ -o public/stylesheets/ -c",
    "watchCSS": "yarn run buildCSS -- --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "stylus": "^0.54.5"
  }
}
